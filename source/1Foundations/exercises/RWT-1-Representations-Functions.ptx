<exercise label="RWT-Representations-Functions-exercise3">
    <statement>
        <p>
            Which graph represents the same function as table below?
        </p>
                <tabular top="major" left="medium" bottom="medium" right="medium">
                    <row halign="center"> 
                        <cell>Input</cell>
                        <cell><m>-3</m></cell>
                        <cell><m>-2</m></cell>
                        <cell><m>-1</m></cell>
                        <cell><m> 0</m></cell>
                        <cell><m> 1</m></cell>
                        <cell><m> 2</m></cell>
                        <cell><m> 3</m></cell>
                    </row>
                    <row halign="center">
                        <cell>Output</cell>
                        <cell><m>9</m></cell>
                        <cell><m>4</m></cell>
                        <cell><m>1</m></cell>
                        <cell><m>0</m></cell>
                        <cell><m>1</m></cell>
                        <cell><m>4</m></cell>
                        <cell><m>9</m></cell>
                    </row>
                </tabular>
    </statement>

   <choices cols="2">
