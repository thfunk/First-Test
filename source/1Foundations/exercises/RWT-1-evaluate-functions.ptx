<exercises>
<exercise label="RWT-Evaluating-Functions-exercise1">
        <statement>
            <p>
                Complete the input/output table for the equation <m>y=<eval obj="theFunction" /></m>. Be sure to simplify your answer.
            </p>
            <table>
                <title>Input/Output Table</title>
                <tabular top="major" left="medium" bottom="medium" right="medium">
                    <row halign="center">
                        <cell>Input</cell>
                        <cell>Output</cell>
                    </row>
                    <row halign="center">
                        <cell><m>-1</m></cell>
                        <cell><fillin width="5" mode="math" ansobj="Answer1" name="response1"/></cell>
                    </row>
                    <row halign="center">
                        <cell><m>0</m></cell>
                        <cell><fillin width="5" mode="math" ansobj="Answer2" name="response2"/></cell>
                    </row>
                    <row halign="center">
                        <cell><m>2</m></cell>
                        <cell><fillin width="5" mode="math" ansobj="Answer3" name="response3"/></cell>
                    </row>
                </tabular>
            </table>
        </statement>
          <setup seed="12345">
    <de-object name="m" context="number">
    <de-random distribution="discrete" nonzero="yes" min="2" max="7" by="1" />
    </de-object>
    <de-object name="a" context="number">
    <de-random distribution="discrete" nonzero="yes" min="2" max="7" by="1" />
    </de-object>
    <de-object name="theFunction" context="formula">
    <de-expression mode="formula" reduce="yes">m(x+a)</de-expression>
    </de-object>
    <de-object name="Answer1" context="formula">
    <de-expression mode="formula" reduce="yes">m*(-1+a)</de-expression>
    </de-object>
    <de-object name="Answer2" context="formula">
    <de-expression mode="formula" reduce="yes">m*a</de-expression>
    </de-object>
    <de-object name="Answer3" context="formula">
    <de-expression mode="formula" reduce="yes">m*(2+a)</de-expression>
    </de-object>
  </setup>
<evaluation>
  <evaluate submit="response1">
  <test correct="yes">
  <mathcmp use-answer="yes" />
  </test>
  </evaluate>
  <evaluate submit="response2">
  <test correct="yes">
  <mathcmp use-answer="yes" />
  </test>
  </evaluate>
  <evaluate submit="response3">
  <test correct="yes">
  <mathcmp use-answer="yes" />
  </test>
  </evaluate>
  </evaluation>
 </exercise>

